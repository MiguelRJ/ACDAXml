<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.acdaxmlclase">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.acdaxmlclase.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.acdaxmlclase.Partes" />
        <activity android:name="com.example.acdaxmlclase.Notas" />
        <activity android:name="com.example.acdaxmlclase.Noticias" />
        <activity android:name="com.example.acdaxmlclase.Titulares" />
        <activity android:name="com.example.acdaxmlclase.EscribirXML"></activity>
    </application>

</manifest>